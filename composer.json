{
    "name": "larafun/suite",
    "description": "A collection of classes and traits that will make you Laravel API workflow even more awesome!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/contracts": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0",
        "orchestra/database": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Larafun\\Suite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larafun\\Suite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larafun\\Suite\\ServiceProvider"
            ]
        }
    }
}

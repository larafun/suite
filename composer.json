{
    "name": "larafun/suite",
    "description": "A collection of classes and traits that will make you Laravel API workflow even more awesome!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/contracts": "5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.8.*",
        "orchestra/database": "3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Larafun\\Suite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larafun\\Suite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larafun\\Suite\\ServiceProvider"
            ]
        }
    }
}

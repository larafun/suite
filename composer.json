{
    "name": "larafun/suite",
    "description": "A collection of classes and traits that will make you Laravel API workflow even more awesome!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "orchestra/database": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Larafun\\Suite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larafun\\Suite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larafun\\Suite\\ServiceProvider"
            ]
        }
    }
}

{
    "name": "larafun/suite",
    "description": "A collection classes and traits that will make you Laravel workflow even more awesome!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/contracts": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0||^7.0",
        "orchestra/testbench": "^3.5",
        "orchestra/database": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Larafun\\Suite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larafun\\Suite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larafun\\Suite\\ServiceProvider"
            ]
        }
    }
}
